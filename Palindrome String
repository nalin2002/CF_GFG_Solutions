class Solution{
public:	
	
	
	int isPlaindrome(string S)
	{
	    /*
	    string rev=string(S.rbegin(),S.rend());
	    if(rev==S) return 1;
	    else return 0;
	    */
	    
	    int n=S.size();
	    for(int i=0,j=n-1;i<j;i++,j--){
	        if(S[i]!=S[j])
	        return 0;
	    }
	    return 1;
	}

};

#include <iostream>
#include <string>
using namespace std;

int main()
{
  int t;
  cin>>t;
  while(t--){
      string s;
      cin>>s;
      string ans;
      ans.push_back(s[0]);
      if(s.size()==2){
          cout<<s<<endl;
      }
      else{
          for(int i=1;i<s.size();i+=2){
              ans.push_back(s[i]);
          }
          cout<<ans<<endl;
      }
  }
   return 0;
}

#include <iostream>
#include <string>
#include <set>
using namespace std;

int main()
{
   string s;
   getline(cin,s);
   int n=s.size();
   set<char> st;
   for(int i=0;i<n;i++){
       if(((int)s[i])>=97 && ((int)s[i])<=122){
           st.insert(s[i]);
       }
   }
   int x=st.size();
   cout<<x<<endl;
   return 0;
}

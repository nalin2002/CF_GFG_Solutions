#include <iostream>
#include <cmath>
#define X 2
#define Y 2
using namespace std;

int main() {
    bool a[5][5];
    int xvalue,yvalue;
    for(int i=0;i<5;i++){
        for(int j=0;j<5;j++){
            cin>>a[i][j];
            if(a[i][j]==1){
                xvalue=i;
                yvalue=j;
            }
        }
    }
int moves=abs(xvalue-X)+abs(yvalue-Y);
cout<<moves<<endl;
	
	return 0;
}

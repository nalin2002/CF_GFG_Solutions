

#include <iostream>
 
using namespace std;
 
int main()
{
   int t;
   cin>>t;
   while(t--){
       int x;
       cin>>x;
       int count=0;
       int a=x;
       int rem;
       while(a){
           rem=a%10;
           a=a/10;
           count++;
       }
       int digit=rem;
       int ans= (digit-1)*10 + ((count)*(count+1)/2);
       cout<<ans<<endl;
   }
   
   return 0;
}




#include <iostream>

using namespace std;

int main()
{
  int t;
  cin>>t;
  while(t--){
      int a,b;
      cin>>a>>b;
      if(a==b){
          cout<<0<<endl;
      }
      else if(a>b){
          int x=(a-b)/10;
          int y=(a-b)%10;
          if(y==0) cout<<x<<endl;
          else cout<<x+1<<endl;
      }
      else{
          int x=(b-a)/10;
          int y=(b-a)%10;
          if(y==0) cout<<x<<endl;
          else cout<<x+1<<endl;
      }
  }
   return 0;
}
